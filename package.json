{
  "name": "snailsvn",
  "displayName": "SnailSvn",
  "description": "SnailSvn helper for Vscode with more actions",
  "version": "1.0.6",
  "engines": {
    "vscode": "^1.80.0"
  },
  "l10n": "./l10n",
  "keywords": [
    "SnailSVN",
    "SVN",
    "newnight"
  ],
  "repository": "https://github.com/newnight/SnailSvn-helper-vscode",
  "publisher": "newnight",
  "categories": [
    "Other"
  ],
  "icon": "images/SnailSvn.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "newnight.snailsvn.resetLocalize",
        "title": "Snailsvn resetLocalize"
      },
      {
        "command": "newnight.snailsvn.update",
        "title": "%Snailsvn Update%"
      },
      {
        "command": "newnight.snailsvn.commit",
        "title": "%Snailsvn Commit%"
      },
      {
        "command": "newnight.snailsvn.revert",
        "title": "%Snailsvn Revert%"
      },
      {
        "command": "newnight.snailsvn.log",
        "title": "%Snailsvn Log%"
      },
      {
        "command": "newnight.snailsvn.add",
        "title": "%Snailsvn Add%"
      },
      {
        "command": "newnight.snailsvn.blame",
        "title": "%Snailsvn Blame%"
      },
      {
        "command": "newnight.snailsvn.delete",
        "title": "%Snailsvn Delete%"
      },
      {
        "command": "newnight.snailsvn.lock",
        "title": "%Snailsvn Lock%"
      },
      {
        "command": "newnight.snailsvn.unlock",
        "title": "%Snailsvn Unlock%"
      },
      {
        "command": "newnight.snailsvn.info",
        "title": "%Snailsvn Info%"
      },
      {
        "command": "newnight.snailsvn.cleanup",
        "title": "%Snailsvn Cleanup%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "newnight.snailsvn.update",
          "when": "editorFocus",
          "group": "snailsvn@1"
        },
        {
          "command": "newnight.snailsvn.commit",
          "when": "editorFocus",
          "group": "snailsvn@2"
        },
        {
          "command": "newnight.snailsvn.log",
          "group": "snailsvn@3"
        },
        {
          "submenu": "newnight.snailsvn",
          "when": "editorFocus",
          "group": "snailsvn@4"
        }
      ],
      "explorer/context": [
        {
          "command": "newnight.snailsvn.update",
          "group": "snailsvn@1"
        },
        {
          "command": "newnight.snailsvn.commit",
          "group": "snailsvn@2"
        },
        {
          "command": "newnight.snailsvn.log",
          "group": "snailsvn@3"
        },
        {
          "submenu": "newnight.snailsvn",
          "group": "snailsvn@4"
        }
      ],
      "newnight.snailsvn": [
        {
          "command": "newnight.snailsvn.info",
          "group": "snailsvn@999"
        },
        {
          "command": "newnight.snailsvn.update",
          "group": "snailsvn@1"
        },
        {
          "command": "newnight.snailsvn.commit",
          "group": "snailsvn@2"
        },
        {
          "command": "newnight.snailsvn.log",
          "group": "snailsvn@3"
        },
        {
          "command": "newnight.snailsvn.add",
          "group": "snailsvn@4"
        },
        {
          "command": "newnight.snailsvn.blame",
          "group": "snailsvn@6"
        },
        {
          "command": "newnight.snailsvn.delete",
          "group": "snailsvn@7"
        },
        {
          "command": "newnight.snailsvn.lock",
          "group": "snailsvn@7"
        },
        {
          "command": "newnight.snailsvn.unlock",
          "group": "snailsvn@8"
        },
        {
          "command": "newnight.snailsvn.revert",
          "group": "snailsvn@5"
        },
        {
          "command": "newnight.snailsvn.cleanup",
          "group": "snailsvn@9"
        }
      ]
    },
    "submenus": [
      {
        "id": "newnight.snailsvn",
        "label": "%Snail Svn%"
      },
      {
        "id": "newnight.snailsvn.explorer",
        "label": "%Snail Svn%"
      }
    ],
    "configuration": [
      {
        "title": "SnailSvn",
        "description": "localize configuration",
        "properties": {
          "SnailSvn.localizeSetting": {
            "type": "object",
            "default": {
              "Snail Svn":"üêå Snail Svn",
              "Snailsvn Update": "‚§µÔ∏è Snailsvn Update",
              "Snailsvn Commit": "‚§¥Ô∏è Snailsvn Commit",
              "Snailsvn Revert": "‚ôªÔ∏è Snailsvn Revert",
              "Snailsvn Log": "üóì Snailsvn Log",
              "Snailsvn Add": "‚ùáÔ∏è Snailsvn Add",
              "Snailsvn Blame": "üëÅ Snailsvn Blame",
              "Snailsvn Delete": "‚ùå Snailsvn Delete",
              "Snailsvn Lock": "üîí Snailsvn Lock",
              "Snailsvn Unlock": "üîë Snailsvn Unlock",
              "Snailsvn Info": "üìù Snailsvn Info",
              "Snailsvn Cleanup": "üóë Snailsvn Cleanup"
            },
            "description": "localize configuration"
          }
        }
      }
    ]
  },
  "scripts": {
    "package": "vsce package --yarn --dependencies",
    "publish": "yarn run package && vsce publish",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/l10n-dev": "^0.0.29",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.16"
  }
}
